              </type>
            </variable>
            <variable name="sum">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[i:=1;
sum := 0;
while (i < 5) do
  sum := sum + IN1;
  i:=i+1;
end_while;
avg:= sum /5;

if avg <=20 then
  OUT_MV2 := FALSE;
  OUT_MV1 := TRUE;
end_if;
if avg >=60 THEN
  OUT_MV2 := TRUE;
  OUT_MV1 := FALSE;
end_if;
]]></xhtml:p>
          </ST>
        </body>
