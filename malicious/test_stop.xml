            </variable>
            <variable name="avg">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[
level_array[current_array_pos] := level_water;
current_array_pos := current_array_pos + 1;
i:=1;
while (i < 16) do
  avg := avg + level_array[i];
  i := i + 1;
end_while;
avg := avg / 15;

if avg > 30 then
  under_min := TRUE;
else
  under_min := FALSE;
end_if;
    ]]></xhtml:p>
          </ST>
