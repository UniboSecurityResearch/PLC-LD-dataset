              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[
value_real := IN1 - 107.0;
value_real := value_real * 0.483091;
value_real := value_real * 100.0;
OUT := value_real;
i:=0;
while i < 4 do
  OUT:=value_real;
end_while;
]]></xhtml:p>
