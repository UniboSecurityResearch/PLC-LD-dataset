              </type>
            </variable>
          </outputVars>
          <localVars>
            <variable name="i">
              <type>
                <INT/>
              </type>
            </variable>
            <variable name="avg">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[i:=1;
while (i < 16) do
  avg := avg + level_water;
end_while;

avg:= avg /15;
if avg > 30 then
  under_min := TRUE;
else
  under_min := FALSE;
end_if;

    ]]></xhtml:p>
          </ST>
        </body>
