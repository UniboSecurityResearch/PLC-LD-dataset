      <pou name="level_filtering" pouType="functionBlock">
        <interface>
          <inputVars>
            <variable name="level_water">
              <type>
                <INT/>
              </type>
            </variable>
          </inputVars>
          <outputVars>
            <variable name="under_min">
              <type>
                <BOOL/>
              </type>
            </variable>
          </outputVars>
          <localVars>
            <variable name="i">
              <type>
                <INT/>
              </type>
            </variable>
            <variable name="current_array_pos">
              <type>
                <INT/>
              </type>
            </variable>
            <variable name="level_array">
              <type>
                <array>
                  <dimension lower="1" upper="20"/>
                  <baseType>
                    <INT/>
                  </baseType>
                </array>
              </type>
            </variable>
            <variable name="avg">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[
level_array[current_array_pos] := level_water;
current_array_pos := current_array_pos + 1;
i:=1;
while (i < 16) do
  avg := avg + level_array[i];
  i := i + 1;
end_while;
avg := avg / 15;

if avg > 30 then
  under_min := TRUE;
else
  under_min := FALSE;
end_if;
    ]]></xhtml:p>
          </ST>
        </body>
